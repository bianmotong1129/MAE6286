    "g = 9.81 \n",
    "vt = 4.9  \n",
    "CD = 1.0 / 5.0 \n",
    "CL = 1.0 \n",
    "\n",
    "v0 = 6.5  \n",
    "theta0 = -0.1  \n",
    "x0 = 0.0  \n",
    "y0 = 2.0  \n",
    "\n",
    "T = 15.0  \n",
    "dt = 0.01  \n",
    "N = int(T / dt) + 1  \n",
    "\n",
    "u_leapfrog = numpy.empty((N, 4))\n",
    "\n",
    "u_leapfrog[0] = numpy.array([v0, theta0, x0, y0])\n",
    "\n",
    "u_leapfrog[1] = rk2_step(u_leapfrog[0], rhs_phugoid, dt, CL, CD, g, vt)"
   ]
